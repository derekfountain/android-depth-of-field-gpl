<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.derekfountain.dofc"
      android:versionCode="1"
      android:versionName="1.0">
      
    <!-- Android 1.6 or better. It might work with earlier, but I haven't
         tested it, and I rely on the graphics scaling introduced in 1.6
    -->
	<uses-sdk android:minSdkVersion="4" />
	<supports-screens android:anyDensity="true" /> 
      
    <application android:icon="@drawable/ic_launcher_dofc" android:label="@string/app_name" android:debuggable="true">
        <activity class=".DepthOfFieldCalc"
                  android:name=".DepthOfFieldCalc"
                  android:label="@string/app_name"
                  android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    <activity android:name="Page"></activity>
</application>


</manifest> 